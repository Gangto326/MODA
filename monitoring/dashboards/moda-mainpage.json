  "panels": [
    {
      "title": "Main Page Response Time (P50 / P95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(moda_mainpage_duration_seconds_bucket[5m]))",
          "legendFormat": "P50",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(moda_mainpage_duration_seconds_bucket[5m]))",
          "legendFormat": "P95",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" },
        "overrides": []
      }
    },
    {
      "title": "Main Page Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "rate(http_server_requests_seconds_count{uri=\"/api/search/main\", application=\"moda-api\"}[5m])",
          "legendFormat": "req/s",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" },
        "overrides": []
      }
    },
    {
      "title": "Search Executor During Main Page",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "executor_active_threads{name=\"searchExecutorService\"}",
          "legendFormat": "active",
          "refId": "A"
        },
        {
          "expr": "executor_queued_tasks{name=\"searchExecutorService\"}",
          "legendFormat": "queued",
          "refId": "B"
        }
      ]
    },
    {
      "title": "Main Page Avg Response",
      "type": "stat",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "rate(moda_mainpage_duration_seconds_sum[5m]) / rate(moda_mainpage_duration_seconds_count[5m])",
          "legendFormat": "avg",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s", "thresholds": { "steps": [{ "value": 0, "color": "green" }, { "value": 0.5, "color": "yellow" }, { "value": 2, "color": "red" }] } },
        "overrides": []
      }
    }
  ],
